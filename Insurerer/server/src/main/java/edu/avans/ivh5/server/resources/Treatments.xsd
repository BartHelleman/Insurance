<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="treatments">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="treatment" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="name"/>
              <xs:element type="xs:string" name="code"/>
              <xs:element type="xs:decimal" name="priceSession"/>
              <xs:element type="xs:integer" name="amountSessions"/>
               <xs:element type="xs:string" name="treatmentStatus"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>